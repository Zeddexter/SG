<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>SCerrarSesion</servlet-name>
        <servlet-class>Modelo.SCerrarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SAgregarCarro</servlet-name>
        <servlet-class>Modelo.SAgregarCarro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SFinalizarCompra</servlet-name>
        <servlet-class>Modelo.SFinalizarCompra</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SVerificarUsuario</servlet-name>
        <servlet-class>Modelo.SVerificarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sAccesosOpciones</servlet-name>
        <servlet-class>Modelo.sAccesosOpciones</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarUsuario</servlet-name>
        <servlet-class>Modelo.RegistrarUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SCerrarSesion</servlet-name>
        <url-pattern>/SCerrarSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SAgregarCarro</servlet-name>
        <url-pattern>/SAgregarCarro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SFinalizarCompra</servlet-name>
        <url-pattern>/SFinalizarCompra</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SVerificarUsuario</servlet-name>
        <url-pattern>/SVerificarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sAccesosOpciones</servlet-name>
        <url-pattern>/sAccesosOpciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarUsuario</servlet-name>
        <url-pattern>/RegistrarUsuario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
